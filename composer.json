{
  "name": "dekar91/yii2-datalayer",
  "type": "yii2-extension",
  "description": "DataLayer integration for the Yii framework",
  "keywords": [
    "yii2",
    "datalayer",
    "helper"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Denis Karakoliuchka"
    }
  ],
  "support": {
    "issue": "https://github.com/dekar91/yii2-datalayer/issues"
  },
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.4",
    "yiisoft/yii2": ">=2.0",
    "ezyang/htmlpurifier": "*",
    "bower-asset/inputmask":" ~3.3.5"
  },
  "require-dev": {
    "yiisoft/yii2-debug": "~2.1.19",
    "yiisoft/yii2-faker": "~2.0.5",
    "codeception/codeception": "4.1.31",
    "codeception/verify": "~2.2.0",
    "codeception/specify": "~2.0.0"
  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  },
  "autoload": {
    "psr-4": {
      "dekar91\\datalayer\\": ""
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
